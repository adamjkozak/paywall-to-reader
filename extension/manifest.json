{
  "manifest_version": 3,
  "name": "Archive Opener",
  "description": "Open the most recent archived version of the current page",
  "version": "1.1",
  "action": {},
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "https://archive.is/*",
    "https://archive.ph/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
